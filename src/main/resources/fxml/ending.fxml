            <BorderPane.margin>
                <Insets bottom="40.0" />
            </BorderPane.margin>
      </HBox>
    </bottom>
    <top>
        <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-font-size: 50;" text="Játék vége" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="20.0" />
            </BorderPane.margin></Label>
    </top>
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="winnerLabel" style="-fx-font-size: 40;" textAlignment="CENTER" underline="true" wrapText="true" />
            <Label style="-fx-font-size: 40;" text="Szép munka!" />
         </children>
      </VBox>
   </center>
</BorderPane>
